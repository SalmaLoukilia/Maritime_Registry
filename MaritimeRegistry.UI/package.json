{
  "name": "maritime-registry.ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:MaritimeRegistry.UI": "node dist/MaritimeRegistry.UI/server/server.mjs",
    "start:api": "cd ../MaritimeRegistry.API && dotnet run",
    "start:ui": "ng serve --open",
    "dev": "concurrently \"npm run start:api\" \"npm run start:ui\"",
    "dev:watch": "concurrently \"npm run start:api\" \"ng serve --open --hmr\""
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.2.1",
    "@angular/common": "^20.2.1",
    "@angular/compiler": "^20.2.1",
    "@angular/core": "^20.2.1",
    "@angular/forms": "^20.2.1",
    "@angular/platform-browser": "^20.2.1",
    "@angular/platform-server": "^20.2.1",
    "@angular/router": "^20.2.1",
    "@angular/ssr": "^20.0.5",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "chart.js": "^3.9.1",
    "countup.js": "^2.9.0",
    "express": "^5.1.0",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "ng2-charts": "^4.1.1",
    "ngx-countup": "^13.1.0",
    "qrcode": "^1.5.4",
    "rxjs": "^7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.0.5",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.0",
    "@types/chart.js": "^2.9.41",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^30.0.0",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^20.17.19",
    "@types/qrcode": "^1.5.5",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "^5.5.4"
  }
}
